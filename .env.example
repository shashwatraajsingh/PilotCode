# API Keys (Optional - BYOK supported)
# Users can provide their own API keys via the frontend UI
# These are fallback keys if users don't provide their own
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GITHUB_TOKEN=

# Database
DATABASE_URL=postgresql://devin:devin_password@localhost:5432/devin_db

# Redis
REDIS_URL=redis://localhost:6379

# Kafka
KAFKA_BROKERS=localhost:1010

# Model Configuration
AI_MODEL=gpt-4-turbo-preview
AI_TEMPERATURE=0.2
MAX_TOKENS=4096

# Sandbox Configuration
USE_DOCKER_SANDBOX=true
DOCKER_SANDBOX_IMAGE=devin-sandbox:latest

# Workflow Configuration
MAX_RETRIES=3
TASK_TIMEOUT=300
MAX_CONCURRENT_TASKS=5

# Backend
PORT=3001
NODE_ENV=development

# Authentication & Security
JWT_SECRET=change-this-to-a-secure-random-string-min-32-characters
JWT_REFRESH_SECRET=change-this-to-another-secure-random-string-min-32-characters
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info
LOG_DIR=logs

# Redis Configuration (alternative to REDIS_URL)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Docker
DOCKER_HOST=unix:///var/run/docker.sock

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:3001
